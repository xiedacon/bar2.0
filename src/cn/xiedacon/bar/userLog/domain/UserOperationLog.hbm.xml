<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="cn.xiedacon.bar.userLog.domain.UserOperationLog" table="useroperationlog">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="operation"></property>
		<property name="time"></property>
		
		<!-- 用户操作记录与用户记录之间多对一关系 -->
		<many-to-one name="userLog" class="cn.xiedacon.bar.userLog.domain.UserLog" column="userlogId" lazy="false"></many-to-one>
		
		<!-- 用户操作记录与操作人之间多对一关系 -->
		<many-to-one name="operator" class="cn.xiedacon.bar.user.domain.User" column="operator" lazy="false"></many-to-one>
		
	</class>
</hibernate-mapping>